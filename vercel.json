{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "prisma": {
          "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
        }
      }
    }
  ],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  },
  "functions": {
    "app/api/admin/upload/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/upload/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
} 